    body
        header(id="header")
            .row.line-title
                .container
                    .col-sm-6.working-time
                        i(class="fa fa-calendar working-time-icon" aria-hidden="true")
                        span Giờ làm việc: T2-CN (9am-21pm)
                    .col-sm-6.header-icon-link
                        a(href="#")
                            i(class="fa fa-facebook-square icon-top" aria-hidden="true")
                        a(href="#")
                            i(class="fa fa-youtube icon-top" aria-hidden="true")

                        a(href="#")
                            i(class="fa fa-google-plus-square icon-top" aria-hidden="true")
                        a(href="#")
                            i(class="fa fa-twitter icon-top" aria-hidden="true")
            .row.header__menu
                nav.navbar.navbar-default.nav-style
                   .container-fluid.header__menu-bg
                        .navbar-header
                            button(type="button" data-toggle="collapse" data-target="#myNavbar"  class="navbar-toggle")
                                span.sr-only Toggle navigation
                                span.icon-bar
                                span.icon-bar
                                span.icon-bar
                            a(href="http://thammihungvuong.com.vn/" class="navbar-brand")
                                img(class="header__logo" src="assets/img/az-logo.png" alt="logo")
                        #myNavbar.collapse.navbar-collapse.navbar-right
                            ul.nav.navbar-nav.nav-style
                                li.active.home
                                    a.home(href="#") TRANG CHỦ
                                li
                                    a(href="#") THẨM MỸ KHUÔN MẶT
                                li
                                    a(href="#") BẢNG GIÁ
                                li
                                    a(href="#") KIẾN THỨC THẨM MỸ
                                li
                                    a(href="#") LIÊN HỆ
                                li.phone
                                    .hotline
                                        .col-md-2
                                            i(class="fa fa-mobile fa-4x" aria-hidden="true")
                                        .col-md-10.hotline-box
                                            span.hotline-text HOTLINE
                                        .col-md-10.clearfix
                                            span.hotline-number 0918 045 516
        //END HEADER

        section#main-slider
            .slider.auto-slider-banner(role="toolbar")
                div
                    a(href="#")
                        img.slider__img.img-responsive(src="assets/img/img1.jpg" alt="image slider")
                div
                    a(href="#")
                        img.slider__img.img-responsive(src="assets/img/img2.jpg" alt="image slider")
                div
                    a(href="#")
