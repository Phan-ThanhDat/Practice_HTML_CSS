        //END SLIDER

        section#appointment
            .container-fluid
                .row
                    .container.appoint-header
                        .col-sm-4
                            .col-sm-10.appoint-header__box.box-left
                                img.img-responsive(src="assets/img/az-logo.png")
                                h4 PHẨU THUẬT GỌT MẶT V LINE
                        .col-sm-4
                            .col-sm-10.col-sm-offset-1.appoint-header__box.box-center
                                img.img-responsive(src="assets/img/az-logo.png")
                                h4 NÂNG MŨI KHÔNG PHẨU THUẬT
                        .col-sm-4
                            .col-sm-10.col-sm-offset-2.appoint-header__box.box-right
                                img.img-responsive(src="assets/img/az-logo.png")
                                h4 NÂNG NGỰC NANO CHIP

                .row.appointment__bg
                    .container.appointment__content
                        .col-md-6.appointment__content__left
                            form
                                .form-group
                                    .row.appointment__content__row
                                        .col-md-6
                                            label(for"name") NAME(*)
                                            input.form-control#name(type="text" name="name" placeholder="Your name")
                                        .col-md-6
                                            label(for"lastname") LAST NAME(*)
                                            input.form-control#lastname(type="text" name="lastname" placeholder="Your last name")
                                    .row.appointment__content__row
                                        .col-md-6
                                            label(for"txt-name") DATE OF BIRTH
                                            input.form-control#datebirth(type="date" name="datebirth" placeholder="Date of birth")
                                        .col-md-6
                                            label(for"gender") GENDER
                                            select#gender.form-control(name="gender")
                                                option(value="") Select gebder
                                                option(value="male") male
                                                option(value="female") female
                                    .row.appointment__content__row
                                        .col-md-6
                                            label(for"email") EMAIL
                                            input.form-control#email(type="email" name="email" placeholder="Email")
                                        .col-md-6
                                            label(for"phone") PHONE
                                            input.form-control#phone(type="text" name="phone" placeholder="Phone")
                                    .row.text-right.border-top
                                        .col-md-12
                                            button.btn.btn-default.btn-appointment(type="summit") NEXT
                        .col-md-6.appointment__content__right
                            .row
                                .icon-circle
                                    img.img-responsive(src="assets/img/icon1.png")
                                .col-sm-9
                                    h5 HEALTH INFOMARTION
                                    .appointment-title
                                    span.appointment-text Lorem Ipsum is simply dummy text of the printing and typesetting industry
                            .row
                                .icon-circle
                                    img.img-responsive(src="assets/img/icon1.png")
                                .col-sm-9
                                    h5 HEALTH INFOMARTION
                                    .appointment-title
                                    span.appointment-text Lorem Ipsum is simply dummy text of the printing and typesetting industry
                            .row
                                .icon-circle
                                    img.img-responsive(src="assets/img/icon1.png")
